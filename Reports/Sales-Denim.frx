        temp1.SetName(sDenim + &quot;_1&quot;);
        temp1.PrintOn = GroupFooter1;
        Report.Dictionary.Totals.Add(temp1);
        
        temp2.ResetAfterPrint = true;
        temp2.Evaluator = Data1;
        temp2.Expression = &quot;[Denim_Finishing.SUM_&quot; + sDenim + &quot;]&quot;;
        temp2.SetName(sDenim + &quot;_2&quot;);
        temp2.PrintOn = GroupFooter2;
        Report.Dictionary.Totals.Add(temp2);
      }
      //MessageBox.Show(Report.Dictionary.Totals.Count.ToString());
    }

    private void Data404_BeforePrint(object sender, EventArgs e)
